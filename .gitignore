# PHPStorm workspace files
.idea/*
!.idea/sasslintPlugin.xml

# Ignore OS-specific files
.DS_Store

# Ignore platform db dumps
*--dump.sql

# Frontend
node_modules/

# misc (needs to be removed)
.vagrant/
/vaultpw.txt

# TYPO3 cache
/var/
/bin/
/vendor/
/.php_cs.cache
/.env

# Exclude web, but allow TYPO3-specific files
/web/*
!/web/favicon.*
# Exclude everything in typo3conf/ except typo3conf itself
/web/typo3conf/*
!/web/typo3conf
!/web/typo3conf/LocalConfiguration.php
!/web/typo3conf/AdditionalConfiguration.php
